{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "scripts": {
    "sass": "node-sass app/styles/ -o app/styles/",
    "sass:watch": "npm run sass && node-sass app/styles/ -wo app/styles/",
    "start": "tsc && concurrently \"npm run sass:watch\" \"npm run tsc:w\" \"npm run lite\"",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "angular2-in-memory-web-api": "0.0.20",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.2",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "bootstrap": "^3.3.7",
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "node-sass": "^3.11.2",
    "typescript": "^2.0.2",
    "typings": "^1.3.2"
  }
}
